{
  "name": "flow-jsx-walk",
  "version": "1.0.0",
  "description": "Parse and walk through a Javascript/JSX code source",
  "author": "Heidi Kasemir",
  "license": "MIT",
  "keywords": [
    "acorn",
    "jsx",
    "walk",
    "parse"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hkasemir/flow-jsx-walk.git"
  },
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.7.5",
    "flow-parser": "^0.64.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015-loose": "^7.0.0",
    "eslint": "^2.7.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.15"
  },
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel index.js walk.js --out-dir lib",
    "build:umd": "webpack index.js dist/acorn-jsx-walk.js && set NODE_ENV=production && webpack index.js dist/acorn-jsx-walk.min.js",
    "lint": "eslint index.js",
    "test": "ava",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  }
}
